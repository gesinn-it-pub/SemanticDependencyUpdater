includes:
	- phpstan-baseline.neon

parameters:
	level: 1
	paths:
		- .
	reportUnmatchedIgnoredErrors: false
	excludePaths:
		- vendor (?)
		- node_modules (?)
		- coverage (?)
	scanDirectories:
		- ../SemanticMediaWiki/includes
		- ../SemanticMediaWiki/src
		- ../SemanticMediaWiki/Tesa
		- ../../includes
